@article{MOSBY201475,
	title = "{A fission fragment detector for correlated fission output studies}",
	journal = "Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment",
	volume = "757",
	pages = "75 - 81",
	year = "2014",
	issn = "0168-9002",
	doi = "https://doi.org/10.1016/j.nima.2014.04.066",
	url = "http://www.sciencedirect.com/science/article/pii/S0168900214004793",
	author = "S. Mosby and F. Tovesson and A. Couture and D.L. Duke and V. Kleinrath and R. Meharchand and K. Meierbachtol and J.M. O×³Donnell and B. Perdue and D. Richman and D. Shields",
	keywords = "Fission, Ionization chamber, Digital data acquisition",
	abstract = "A digital data acquisition system has been combined with a double Frisch gridded ionization chamber for use at both moderated and unmoderated neutron sources at the Los Alamos Neutron Science (LANSCE) facility. The high efficiency of the instrument combined with intense LANSCE beams and new acquisition system permits fission output measurements across 11 orders of magnitude incident neutron energy. The acquisition and analysis system is presented along with the first in-beam performance tests of the setup."
}
